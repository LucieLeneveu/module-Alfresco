<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- Registration of new models -->
    <bean id="cd76_dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
	    <property name="models">
	        <list>
	            <value>alfresco/module/cd76/model/CustomFolderProperties.xml</value>
	            <value>alfresco/module/cd76/model/CustomFileProperties.xml</value>
	        </list>
	    </property>
	</bean>
    
</beans>
